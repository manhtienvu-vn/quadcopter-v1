/* Private user code ---------------------------------------------------------*/
/* USER CODE BEGIN 0 */
float round2(float value){
	return roundf(value * 100.0f)/100.0f;
}

uint8_t check;
uint8_t mData;
HAL_StatusTypeDef status;

void imu_init(void){
	status = HAL_I2C_Mem_Read(&hi2c1, MPU6050_ADDRESS, 0x75, 1, &check, 1, 1000);

	if (check == 0x68 || check == 0x70){
		mData = 0x00;
		HAL_I2C_Mem_Write(&hi2c1, MPU6050_ADDRESS, 0x6B, 1, &mData, 1, 1000);

		mData = 0x07;
		HAL_I2C_Mem_Write(&hi2c1, MPU6050_ADDRESS, 0x19, 1, &mData, 1, 1000);

		mData = 0x00;
		HAL_I2C_Mem_Write(&hi2c1, MPU6050_ADDRESS, 0x1B, 1, &mData, 1, 1000);

		mData = 0x00;
		HAL_I2C_Mem_Write(&hi2c1, MPU6050_ADDRESS, 0x1C, 1, &mData, 1, 1000);
	}
}
void getAccel(void){
	uint8_t accelData[6];

	HAL_I2C_Mem_Read(&hi2c1, MPU6050_ADDRESS, 0x3B, 1, accelData, 6, 1000);

	raw_ax = (int16_t)(accelData[0] << 8) | (accelData[1]);
	raw_ay = (int16_t)(accelData[2] << 8) | (accelData[3]);
	raw_az = (int16_t)(accelData[4] << 8) | (accelData[5]);

	AX = (float)raw_ax/16384.0f;
	AY = (float)raw_ay/16384.0f;
	AZ = (float)raw_az/16384.0f;
}

void getGyro(void){
	uint8_t gyroData[6];
	HAL_I2C_Mem_Read(&hi2c1, MPU6050_ADDRESS, 0x43, 1, gyroData, 6, 1000);

	raw_gx = (int16_t)(gyroData[0] << 8) | (gyroData[1]);
	raw_gy = (int16_t)(gyroData[2] << 8) | (gyroData[3]);
	raw_gz = (int16_t)(gyroData[4] << 8) | (gyroData[5]);

	GX = (float)raw_gx/131.0f ;
	GY = (float)raw_gy/131.0f ;
	GZ = (float)raw_gz/131.0f ;
}

void calibrate(void){
	float ag[] = {0, 0, 0, 0, 0, 0};

		for(int i = 0; i < 2000; i++){
			getAccel();
			getGyro();
			ag[0] += AX;
			ag[1] += AY;
			ag[2] += AZ;
			ag[3] += GX;
			ag[4] += GY;
			ag[5] += GZ;
			HAL_Delay(1);
		}

		accelX_offsets = ag[0]/2000.0f;
		accelY_offsets = ag[1]/2000.0f;
		accelZ_offsets = ag[2]/2000.0f;
		gyroX_offsets = ag[3]/2000.0f;
		gyroY_offsets = ag[4]/2000.0f;
		gyroZ_offsets = ag[5]/2000.0f;
}

void getFullReadings( ){
	uint8_t accelBuffer[6];
	HAL_I2C_Mem_Read(&hi2c1, MPU6050_ADDRESS, MPU6050_ACCEL_OUT_REGISTER, 1, accelBuffer, 6, 1000);

	raw_ax = (int16_t)(accelBuffer[0] << 8) | (accelBuffer[1]);
	raw_ay = (int16_t)(accelBuffer[2] << 8) | (accelBuffer[3]);
	raw_az = (int16_t)(accelBuffer[4] << 8) | (accelBuffer[5]);

	accelX = ((float)raw_ax/TEMP_LSB_OFFSET); //- accelX_offsets;
	accelY = ((float)raw_ay/TEMP_LSB_OFFSET); //- accelY_offsets;
	accelZ = ((float)raw_az/TEMP_LSB_OFFSET);
	//-------------------------------------------------------------------------------------------------------
	uint8_t gyroBuffer[6];
	HAL_I2C_Mem_Read(&hi2c1, MPU6050_ADDRESS, MPU6050_GYRO_OUT_REGISTER, 1, gyroBuffer, 6, 1000);

	raw_gx = (int16_t)(gyroBuffer[0] << 8) | (gyroBuffer[1]);
	raw_gy = (int16_t)(gyroBuffer[2] << 8) | (gyroBuffer[3]);
	raw_gz = (int16_t)(gyroBuffer[4] << 8) | (gyroBuffer[5]);

	gyroX = ((float)raw_gx/TEMP_LSB_2_DEGREE); //- gyroX_offsets;
	gyroY = ((float)raw_gy/TEMP_LSB_2_DEGREE); //- gyroY_offsets;
	gyroZ = ((float)raw_gz/TEMP_LSB_2_DEGREE); //- gyroZ_offsets;
}

void GetFilteredData(float AX, float AY, float AZ, float GX, float GY, float GZ, float filterGyroCoef){
	currentTime1 = __HAL_TIM_GET_COUNTER(&htim2);
	dt = (float)(currentTime1 - previousTime1);
	dt = (dt)/1000000.0f;
	previousTime1 = currentTime1;

	float raw_gx = GX + gyroX_offsets;
	float raw_ay = AY + accelY_offsets;
	float raw_ax = AX + accelX_offsets;

//	float newAngleXGyro = angleX + (GX)*(dt);
	float newAngleXGyro = angleX + (GX)*(dt);
	float newAngleYGyro = angleY + (GY)*(dt);

//	float newAngleXAccel = atan2(AY, sqrt(AX*AX + AZ*AZ)) * RAD_2_DEG;
	float newAngleXAccel = atan2(AY, sqrt(AX*AX + AZ*AZ)) * RAD_2_DEG;
	float newAngleYAccel = atan2(AX, sqrt(AY*AY + AZ*AZ)) * RAD_2_DEG;

	angleX = (filterGyroCoef)*newAngleXGyro + (1.0f-(filterGyroCoef))*newAngleXAccel;
	angleY = (filterGyroCoef)*newAngleYGyro + (1.0f-(filterGyroCoef))*newAngleYAccel;
	angleZ += (gyroZ)*(dt);
}


HAL_StatusTypeDef I2C_CheckDeviceStatus(){
	status = HAL_I2C_IsDeviceReady(&hi2c2, MPU6050_ADDRESS, 20, 100);
	return status;
}

void reset_I2C(I2C_HandleTypeDef *hi2c){
	GPIO_InitTypeDef GPIO_InitStruct = {0};
	status = HAL_I2C_IsDeviceReady(hi2c, MPU6050_ADDRESS, 20, 100);

	if (status != HAL_OK){
		//1. Deinitialize I2C peripheral
		HAL_I2C_DeInit(hi2c);

		//2. Configure both SCL and SDA as GPIO open-drain outputs
		GPIO_InitStruct.Mode = GPIO_MODE_OUTPUT_OD;
		GPIO_InitStruct.Pull = GPIO_NOPULL;
		GPIO_InitStruct.Speed = GPIO_SPEED_LOW;
		GPIO_InitStruct.Pin = GPIO_PIN_10 | GPIO_PIN_11; //SCL and SDA
		HAL_GPIO_Init(GPIOB, &GPIO_InitStruct);

		//3. By default, set both SCL and SDA pin to HIGH level
		HAL_GPIO_WritePin(GPIOB, GPIO_PIN_6, GPIO_PIN_SET); //SCL
		HAL_GPIO_WritePin(GPIOB, GPIO_PIN_7, GPIO_PIN_SET); //SDA
		//--> If the I2C data buses is BUSY (stuck), SDA will be constantly pulled LOW by a slave
		HAL_Delay(2);

		//--> So, we gives the slave some chances to release SDA to idle state and recover the data buses
		for (int i = 0; i < 20; i++) {
			//4.1. If SCL's pulled HIGH and SDA's pulled HIGH
			//--> Then, the data buses now are released to Idle State
			//--> It means data buses now are no longer stuck
			if(HAL_GPIO_ReadPin(GPIOB,GPIO_PIN_7) == GPIO_PIN_SET){
				break;
			}
			//4.2. Pull the SCL pin to LOW -> Gives slave a chance to release SDA
			//The reason is that slave can only change SDA state when SCL is pulled LOW
			HAL_GPIO_WritePin(GPIOB, GPIO_PIN_6, GPIO_PIN_SET);
			HAL_Delay(2);
			//5. Now, pull the SCL pin to HIGH again to continue to check Step 4.1
			HAL_GPIO_WritePin(GPIOB, GPIO_PIN_6, GPIO_PIN_RESET);
			HAL_Delay(2);
		}

		//6. After successful recovering SDA to its idle state, we send 'STOP' signal to all slaves
		//The principle is: change SDA state from LOW to HIGH while SCL is pulled HIGH
		HAL_GPIO_WritePin(GPIOB, GPIO_PIN_6, GPIO_PIN_SET); //SCL is pulled HIGH
		HAL_GPIO_WritePin(GPIOB, GPIO_PIN_7, GPIO_PIN_RESET); //SDA from LOW
		HAL_Delay(1);
		HAL_GPIO_WritePin(GPIOB, GPIO_PIN_7, GPIO_PIN_SET); //SDA to HIGH
		HAL_Delay(2);

		//7. Reinitialize the I2C peripheral
		MX_I2C1_Init();

		//8. Check data buses state again for debugging
		status = HAL_I2C_IsDeviceReady(hi2c, MPU6050_ADDRESS, 20, 100);
		if (status == HAL_OK){
			return;
		}
		else{}
	}
}

uint8_t msg[] = "Vu Tien Manh\n";
uint8_t number = 123;
uint8_t numarray[30];

void sendUART(){
	sprintf((char*)numarray, "Angle X: %d | Angle Y: %d\n", (int)angleX, (int)angleY);
	HAL_UART_Transmit(&huart6, numarray, strlen((char*)numarray), 500);
}

bool flag = false;


